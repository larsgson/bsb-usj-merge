:[{"type":"char","marker":"fr","content":["7:9"]},{"type":"char","marker":"ft","content":["Hebrew ",{"type":"char","marker":"fq","content":["palate"]},""]}],"caller":"+"},"like the finest wine."]},{"type":"para","marker":"s1","content":["<p class=|ihdg|>The Bride"]},{"type":"para","marker":"q1","content":["May it flow smoothly to my beloved,"]},{"type":"para","marker":"q2","content":["gliding gently over lips and teeth.",{"type":"note","marker":"f","content":[{"type":"char","marker":"fr","content":["7:9"]},{"type":"char","marker":"ft","content":["LXX, Syriac, and Vulgate; Hebrew ",{"type":"char","marker":"fq","content":["gliding gently over lips as we sleep"]},""]}],"caller":"+"}]},{"type":"para","marker":"q1","content":[{"type":"verse","marker":"v","number":"10"},"I belong to my beloved,"]},{"type":"para","marker":"q2","content":["and his desire is for me."]},{"type":"para","marker":"q1","content":[{"type":"verse","marker":"v","number":"11"},"Come, my beloved,"]},{"type":"para","marker":"q2","content":["let us go to the countryside;"]},{"type":"para","marker":"q2","content":["let us spend the night among the wildflowers.",{"type":"note","marker":"f","content":[{"type":"char","marker":"fr","content":["7:11"]},{"type":"char","marker":"ft","content":["Or ",{"type":"char","marker":"fq","content":["among the henna blossoms"]}," or ",{"type":"char","marker":"fq","content":["in the villages"]},""]}],"caller":"+"}]},{"type":"para","marker":"q1","content":[{"type":"verse","marker":"v","number":"12"},"Let us go early to the vineyards"]},{"type":"para","marker":"q2","content":["to see if the vine has budded,"]},{"type":"para","marker":"q1","content":["if the blossom has opened,"]},{"type":"para","marker":"q2","content":["if the pomegranates are in bloom—"]},{"type":"para","marker":"q2","content":["there I will give you my love."]},{"type":"para","marker":"q1","content":[{"type":"verse","marker":"v","number":"13"},"The mandrakes send forth a fragrance,"]},{"type":"para","marke